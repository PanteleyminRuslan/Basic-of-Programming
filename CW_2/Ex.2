#include<iostream>
#include<iomanip>
using namespace std;

int main()
{
  int A[100][100];
  int n;
  cin >> n;
  for (int i = 0; i <n; i++)
  {
    for (int j = 0; j < n; j++)
    {
      // cin>>A[i][j];
      A[i][j] = rand() % 50;
    }
  }

  for (int i = 0; i < n; i++)
  {
    for (int j = 0; j < n; j++)
    {
      cout << setw(5) << A[i][j];
    }
    cout << endl;
  }
  cout << endl;
  
  double b;
  for (int i = 0; i < n; i++)
  {
    b = A[i][n - i-1];
    A[i][n - i-1] = A[i][i];
    A[i][i] = b;
  }
  for (int i = 0; i < n; i++)
  {
    for (int j = 0; j < n; j++)
    {
      cout << setw(5) << A[i][j];
    }
    cout << endl;
  }


}
